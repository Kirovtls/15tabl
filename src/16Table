CREATE TABLE uzer(
   inn INT PRIMARY KEY,
    name VARCHAR(20),
    age INTEGER
);
INSERT INTO uzer(inn, name, age) values (111,'MAKS',18);

CREATE TABLE uzer1(
    inn INT PRIMARY KEY,
    name VARCHAR(33),
    age INTEGER,
    id_uzer INT REFERENCES uzer(inn)

);
INSERT INTO uzer1(inn, name, age) values (222,'Aziza',40);

